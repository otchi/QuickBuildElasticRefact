    {
        _config:{  
            _host:"http://localhost:9200",
            _indexes:{
                    "vehicule":["voiture"]
                },
            
            _request:{
                class:"com.edifixio.amine.beans.RequestBean",
                alias:{
                    query:"query::filtered::query",
                    voiture:"$$query::term::voiture"
                },
                mapping:{
                    mainSearch:["$$voiture::value"]
                        }
            },
            
            
            _meta_response:{
                    class:"com.edifixio.amine.config.Mapping",
                    alias:{tt:"lala"},
                    mapping:{mainSearch:"$$voiture::value"}
            },
            _response:{
                    class:"com.edifixio.amine.beans.ResponseBean",
                    mapping:{
                        id:"$$id",
                        voiture:"$$voiture",
                        MPG:"$$MPG",
                        cylendres:"$$cylendres",
                        vitesseMax:"$$vitesseMax",
                        nbrChauveaux:"$$nbrChauveaux",
                        poid:"$$poid",
                        acceleration:"$$acceleration",
                        model:"$$model",
                        origine:"$$origine"
                        
                        
                    }
                }
            ,
            _facets:[ "origine"]
        },
        
        
        _query:{
             "query":  
                {"filtered":{
                    "query": {
                        "term": {
                            "voiture": {
                                "value": "??"
                            }
                        }
                    },
                    "filter":{
                 
                        }
                    }
                }
            
        , "size": 40
            
        ,"aggs": {
            "origine": {
                "terms": {
                    "field": "origine",
                    "size": 10
                }
            }
        }
    }
}